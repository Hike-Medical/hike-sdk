{
  "name": "@hike/ui",
  "version": "1.0.0",
  "description": "Hike SDK enables using our services for manufacturing prosthetics and orthotics medical devices with our AI and 3D printing technology.",
  "keywords": [
    "insoles",
    "medical",
    "foot",
    "health",
    "orthotics",
    "prosthetics",
    "healthcare"
  ],
  "author": "support@hikemedical.com",
  "license": "Apache-2.0",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build && tsc --project tsconfig.cjs.json",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../../.gitignore",
    "clean": "rm -rf node_modules dist",
    "dev:email": "email dev"
  },
  "dependencies": {
    "@hike/services": "workspace:*",
    "@hike/types": "workspace:*",
    "@hike/utils": "workspace:*",
    "@react-email/components": "0.0.25",
    "@react-email/render": "1.0.1"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "^3.0.1"
  }
}
