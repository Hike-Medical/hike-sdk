{
  "name": "@hike/sdk",
  "version": "1.0.0",
  "description": "Hike SDK enables using our services for manufacturing prosthetics and orthotics medical devices with our AI and 3D printing technology.",
  "keywords": [
    "insoles",
    "medical",
    "foot",
    "health",
    "orthotics",
    "prosthetics",
    "healthcare"
  ],
  "author": "support@hikemedical.com",
  "license": "Apache-2.0",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./ui": {
      "import": "./dist/ui/index.js",
      "require": "./dist/cjs/ui/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build && tsc --project tsconfig.cjs.json",
    "dev": "tsc --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../../.gitignore",
    "clean": "rm -rf node_modules dist",
    "test": "jest"
  },
  "dependencies": {
    "@hike/services": "workspace:*",
    "@hike/types": "workspace:*",
    "@hike/ui": "workspace:*",
    "@hike/utils": "workspace:*",
    "@react-email/components": "0.0.36",
    "@react-email/render": "1.0.6",
    "@tabler/icons-react": "^3.31.0",
    "react-email": "^4.0.7",
    "jose": "^6.0.10",
    "dayjs": "^1.11.13",
    "stripe": "^16.2.0"
  },
  "peerDependencies": {
    "@mantine/core": "^7.17.4",
    "@mantine/hooks": "^7.17.4",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "react": "^19",
    "react-dom": "^19",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "testRegex": ".+\\.test\\.ts$",
    "preset": "ts-jest"
  }
}
