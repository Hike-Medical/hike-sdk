    "@types/lodash": "^4.17.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "react": "^19",
    "react-dom": "^19",
    "typescript": "^5",
    "@hike/prettier-config": "workspace:*",
    "@hike/tsconfig": "workspace:*"
