{
  "name": "@hike/utils",
  "version": "1.0.0",
  "description": "Hike SDK enables using our services for manufacturing prosthetics and orthotics medical devices with our AI and 3D printing technology.",
  "keywords": [
    "insoles",
    "medical",
    "foot",
    "health",
    "orthotics",
    "prosthetics",
    "healthcare"
  ],
  "author": "support@hikemedical.com",
  "license": "Apache-2.0",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc --build && npx tsc --project tsconfig.cjs.json",
    "dev": "npx tsc --watch",
    "lint": "npx eslint .",
    "format": "npx prettier --check . --ignore-path ../../../.gitignore",
    "clean": "rm -rf node_modules dist",
    "test": "jest"
  },
  "dependencies": {
    "@hike/types": "workspace:*"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "testRegex": ".+\\.test\\.ts$",
    "preset": "ts-jest"
  }
}
